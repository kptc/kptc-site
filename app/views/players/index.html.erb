<div class="row">
  <div class=col-xs-24>
    <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Phone</th>
          <th>Role</th>
        </tr>
      </thead>
      <tbody>
        <% @players.each do |player| %>
          <tr>
            <td><%= player.full_name %></td>
            <td><%= player.email %></td>
            <td><%= player.phone %></td>
            <td><%= player.role %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>